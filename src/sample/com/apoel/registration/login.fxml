<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="500.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.com.apoel.registration.LogInController">
   <children>
      <Label layoutX="66.0" layoutY="51.0" text="Login: " />
      <PasswordField layoutX="66.0" fx:id="password" layoutY="200.0" promptText="password" />
      <Label layoutX="66.0" layoutY="163.0" text="Password:" />
      <TextField layoutX="66.0" fx:id="login" layoutY="90.0" promptText="login" />
      <Button layoutX="67.0" fx:id="loginButton" onAction="#logIn" layoutY="273.0" mnemonicParsing="false" text="Log in" />
      <Label layoutX="71.0" layoutY="372.0" text="Does not have account ?" />
      <Button layoutX="247.0" fx:id="registrationButton" onAction="#goToRegistration" layoutY="367.0" mnemonicParsing="false" text="Register" />
   </children>
</AnchorPane>
